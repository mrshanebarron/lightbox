{
    "name": "mrshanebarron/lightbox",
    "description": "Image lightbox gallery component for Laravel",
    "keywords": ["laravel", "lightbox", "gallery", "image", "livewire", "vue"],
    "license": "MIT",
    "authors": [{"name": "Shane Barron", "email": "mrshanebarron@gmail.com"}],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {"LaravelDesign\\lightbox\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"LaravelDesign\\lightbox\\Tests\\": "tests/"}
    },
    "extra": {
        "laravel": {
            "providers": ["LaravelDesign\\lightbox\\lightboxServiceProvider"]
        }
    },
    "config": {"sort-packages": true},
    "minimum-stability": "stable",
    "prefer-stable": true
}
